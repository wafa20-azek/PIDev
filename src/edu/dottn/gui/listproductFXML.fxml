         </children></AnchorPane>
   </top>
   <center>
       <javafx.scene.control.ScrollPane style="-fx-background-color: transparent;">
      <AnchorPane fx:id="feed" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
      </javafx.scene.control.ScrollPane>
   </center>
   <left>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btnhome" layoutY="38.0" mnemonicParsing="false" onAction="#home" prefHeight="70.0" prefWidth="200.0" text="Home" />
